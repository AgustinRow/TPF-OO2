{
  "manifest_version": 2,
  "name": "TPF-OO2",
  "version": "1.0",

  "description": "Places a marker with the position a result in Google appears on duckduckgo for the same search term.",

  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/search?*",
        "*://*.duckduckgo.com/?q=*",
        "*://*.bing.com/search?q=*"
      ],
      "js": ["content.js", "node_modules/lodash/lodash.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["<all_urls>"]
}
